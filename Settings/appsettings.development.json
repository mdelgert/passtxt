{
    "MyAppTemplate": {
        "Message": "HelloWorld!",
        "MessageCount": 1,
        "TimeoutInSeconds": 3,
        "IsFeatureEnabled": true
    },
    "Serilog": {
        "MinimumLevel": "Information",
        "WriteTo": [{
                "Name": "Console"
            }, {
                "Name": "File",
                "Args": {
                    "path": "logs/passtxt.log",
                    "rollingInterval": "Day",
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] {Message}{NewLine}{Exception}"
                }
            }
        ]
    }
}
